    <tags>ignore gitignore glob</tags>
    <repository type="git" url="https://github.com/markashleybell/MAB.DotIgnore.git" />
    <dependencies>
      <group targetFramework=".NETFramework4.0" />
      <group targetFramework=".NETStandard1.3">
        <dependency id="NETStandard.Library" version="1.6.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>